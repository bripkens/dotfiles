"*":
  "advanced-open-file":
    createFileInstantly: true
    defaultInputValue: "Project root"
  "atom-css-unit-converter":
    comments: false
    leadingZero: false
  core:
    audioBeep: false
    closeDeletedFileTabs: true
    telemetryConsent: "no"
    themes: [
      "pristine-ui"
      "one-dark-syntax"
    ]
    titleBar: "hidden"
    warnOnLargeFileLimit: 4
  editor:
    autoIndentOnPaste: false
    fontFamily: "Fira Code"
    fontSize: 13
    preferredLineLength: 120
    showIndentGuide: true
  "exception-reporting":
    userId: "20c1b373-4a8d-4198-848f-a3051bafbe9a"
  jsformat:
    format_on_save: false
  linter:
    lintOnChange: false
  "linter-js-standard":
    checkStyleDevDependencies: true
  "linter-ui-default":
    panelHeight: 81
  "prettier-atom":
    prettierOptions:
      printWidth: 120
      singleQuote: true
      tabWidth: 2
  "pristine-ui":
    tabSizing: "Minimum"
  "svg-preview":
    openPreviewAutomatically: true
  tabs:
    enableVcsColoring: true
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
  zentabs:
    maximumOpenedTabs: 20
    neverCloseDirty: true
    neverCloseUnsaved: true
